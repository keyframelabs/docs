{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Keyframe Labs",
  "colors": {
    "primary": "#81787A",
    "light": "#CFCDC7",
    "dark": "#342E2E"
  },
  "favicon": "/favicon.png",
  "navigation": {
    "global": {
      "anchors": [
        {
          "anchor": "Playground",
          "icon": "laptop",
          "href": "https://playground.keyframelabs.com"
        }
      ]
    },
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting started",
            "expanded": true,
            "pages": [
              "index",
              "guides/getting-started/quickstart",
              "guides/getting-started/dashboard"
            ]
          },
          {
            "group": "Integration",
            "expanded": true,
            "pages": [
              "guides/integration/overview",
              {
                "group": "Agent frameworks",
                "expanded": true,
                "pages": [
                  "guides/integration/agent-frameworks/elevenlabs",
                  "guides/integration/speech-to-video/cartesia",
                  "guides/integration/speech-to-video/gemini",
                  "guides/integration/speech-to-video/open-ai"
                ]
              }
            ]
          }
        ]
      },
      {
        "tab": "API reference",
        "openapi": "/api-reference/v1/openapi.json"
      },
      {
        "tab": "SDK reference"
      }
    ]
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg",
    "href": "https://keyframelabs.com"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@keyframelabs.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://dashboard.mintlify.com"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "github": "https://github.com/keyframelabs",
      "linkedin": "https://www.linkedin.com/company/keyframe-labs-inc",
      "website": "https://keyframelabs.com"
    }
  },
  "background": {
    "color": {
      "light": "#FFFFFF",
      "dark": "#000000"
    }
  }
}
